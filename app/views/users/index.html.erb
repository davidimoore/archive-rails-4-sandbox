<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <%= render partial: 'user', locals: { user: user } %>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
